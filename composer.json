{
    "name": "miladimos/create-laravel-package",
    "description": "The Laravel Zero Framework.",
    "keywords": [
        "framework",
        "laravel",
        "laravel zero",
        "console",
        "cli"
    ],
    "homepage": "https://create-laravel-package.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/miladimos/create-laravel-package/issues",
        "source": "https://github.com/miladimos/create-laravel-package"
    },
    "authors": [
        {
            "name": "miladimos",
            "email": "message.tome1331@gmail.com",
            "role": "creator"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^6.5.5",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/filesystem": "^7.0|^8.0",
        "illuminate/http": "^8.0",
        "illuminate/log": "^8.0",
        "illuminate/support": "^7.0|^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/framework": "^8.0",
        "nunomaduro/laravel-desktop-notifier": "^2.5",
        "padraic/phar-updater": "^1.0.6",
        "symfony/process": "^5.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "create-laravel-package"
    ]
}
